{
  "name": "resin-device-status",
  "version": "2.1.1",
  "description": "Resin.io device status interpreter",
  "main": "build/status.js",
  "types": "typings/resin-device-status.d.ts",
  "homepage": "https://github.com/resin-io/resin-device-status",
  "repository": {
    "type": "git",
    "url": "git://github.com/resin-io/resin-device-status.git"
  },
  "files": [
    "build/",
    "typings/"
  ],
  "keywords": [
    "resin",
    "logs",
    "device"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "npm run test-node && npm run test-browser",
    "test-node": "gulp test",
    "test-browser": "karma start",
    "build": "gulp build",
    "lint": "gulp lint",
    "prepublish": "npm test && npm run build",
    "readme": "jsdoc2md --template doc/README.hbs build/status.js > README.md"
  },
  "author": "Juan Cruz Viotti <juan@resin.io>",
  "license": "Apache-2.0",
  "devDependencies": {
    "coffeescript": "~1.12.2",
    "gulp": "^3.8.10",
    "gulp-coffee": "^2.3.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-mocha": "^3.0.0",
    "gulp-util": "^3.0.1",
    "jsdoc-to-markdown": "^1.1.1",
    "karma": "^3.0.0",
    "mocha": "^5.2.0",
    "mochainon": "^2.0.0",
    "resin-config-karma": "^2.2.0"
  },
  "dependencies": {
    "lodash": "^4.0.0"
  }
}
